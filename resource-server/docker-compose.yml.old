version: '3.1'
services:
  app:
    container_name: resource-server
    image: resource-server
    build: ./
    ports:
      - 8081:8081
    depends_on:
      - postgres
  postgres:
    image: "postgres:9.6.11-alpine"
    ports:
      - "5433:5432"
    environment:
      - POSTGRES_PASSWORD=boehringer
      - POSTGRES_USER=boehringer
      - POSTGRES_DB=boehringer


